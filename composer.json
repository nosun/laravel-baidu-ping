{
  "name": "larva/laravel-baidu-ping",
  "description": "This is a baidu ping.",
  "keywords": [ "laravel", "baidu", "larva" ],
  "type": "library",
  "license": "MIT",
  "require": {
    "ext-bcmath": "*",
    "ext-json": "*",
    "illuminate/bus": "^5.7 || ^6.0",
    "illuminate/events": "^5.7 || ^6.0",
    "illuminate/http": "^5.7 || ^6.0",
    "illuminate/queue": "^5.7 || ^6.0",
    "illuminate/database": "^5.7 || ^6.0",
    "illuminate/support": "^5.7 || ^6.0",
    "larva/site-tool": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Larva\\Baidu\\Ping\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Larva\\Baidu\\Ping\\BaiduPingServiceProvider"
      ]
    }
  },
  "prefer-stable": true
}
