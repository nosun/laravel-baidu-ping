{
  "name": "larva/laravel-baidu-ping",
  "description": "This is a baidu ping.",
  "keywords": [ "laravel", "baidu", "larva" ],
  "type": "library",
  "license": "MIT",
  "require": {
    "ext-json": "*",
    "illuminate/bus": "^6.0 || ^7.0",
    "illuminate/events": "^6.0 || ^7.0",
    "illuminate/http": "^6.0 || ^7.0",
    "illuminate/queue": "^6.0 || ^7.0",
    "illuminate/database": "^6.0 || ^7.0",
    "illuminate/support": "^6.0 || ^7.0",
    "larva/site-tool": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Larva\\Baidu\\Ping\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Larva\\Baidu\\Ping\\BaiduPingServiceProvider"
      ]
    }
  },
  "prefer-stable": true
}
